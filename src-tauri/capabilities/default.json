{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://github.com/*"
        }
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*:*"
        },
        {
          "url": "https://*:*"
        }
      ],
      "deny": []
    },
    "fs:default",
    "log:default"
  ]
}